
<td align="center">
  <a rel="nofollow" data-method="delete" href='/questions/' id.to_s >Remove</a>
</td>
<td>
  <input size="6" value="' + seq.to_s " name="question[' + id.to_s + '][seq]" id="question_' + id.to_s + '_seq" type="text">
</td>
<td>
  <textarea cols="50" rows="1" name="question[' + id.to_s + '][txt]" id="question_' + id.to_s + '_txt" placeholder="Edit question content here">' + txt + '</textarea>
</td>
<td>
  <input size="10" disabled="disabled" value="' + type " name="question[' + id.to_s + '][type]" id="question_' + id.to_s + '_type" type="text">
</td>
<td> alternatives
  <input size="8" value="' + alternatives " name="question[' + id.to_s + '][alternatives]" id="question_' + id.to_s + '_alternatives" type="text">
</td>